{
  "name": "compretend",
  "version": "0.1.0",
  "description": "",
  "main": "components/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "tap test/test-*.js",
    "demos": "micro service/api.js & budo dist.js:bundle.js -d demos",
    "posttest": "standard",
    "coverage": "tap test/test-*.js --coverage-report=lcov",
    "postcoverage": "codecov",
    "precommit": "npm test",
    "prepush": "npm test",
    "prepublish": "mkdir -p dist && browserify dist.js > dist/compretend.js && cat dist/compretend.js | minify > dist/compretend.min.js",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "micro service/api.js",
    "devsocket": "micro service/api.js & budo index.js:bundle.js"
  },
  "standard": {
    "ignore": ["dist/"]
  },
  "keywords": [],
  "author": "Mikeal Rogers <mikeal.rogers@gmail.com> (http://www.mikealrogers.com)",
  "license": "Apache-2.0",
  "dependencies": {
    "blob-util": "^1.3.0",
    "canvas": "^1.6.7",
    "drag-drop": "^2.13.2",
    "micro": "^9.0.0",
    "multihasher": "^1.0.0",
    "opencv": "git+https://github.com/peterbraden/node-opencv.git#52333d4",
    "tap": "^10.7.2",
    "zcomponent": "^1.0.4",
    "zombie": "^5.0.7"
  },
  "devDependencies": {
    "babel-minify": "^0.2.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.4",
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.3",
    "load-js": "^2.0.0",
    "now": "^8.3.5",
    "request-promise": "^4.2.1",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "standard": "^10.0.3",
    "validate-commit-msg": "^2.13.1"
  }
}
